extends ../layouts/default
include ../mixins/cta

block intro
	section.intro.green.text-center
		h1= data.privacypolicy.title
		h3
	section.grey
		ul.container.breadcrumb
			li= data.privacypolicy.breadcrumbstitle
block content
	section.p3rem
		.container
			.row
				.col-md-12.text-center
					h2= data.privacypolicy.title
				.col-md-12
					div!= data.privacypolicy.content
	+cta
