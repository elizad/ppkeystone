extends ../../layouts/default

block content
	.container
		h1 PRODUCTS
		.row
			.col
				if products.length
					each product in products
						h2= product.title
						a(href='http://localhost:3000/products/'+product.slug)
							span= product.title
					
				else
					h3.text-muted There are no name yet.
