extends ../layouts/default
include ../mixins/cta
block metadata
	if  (data.processes)
		title= data.processes.metadata.metatitle || 'Profile Pension'
		meta(property='og:title' , content='' + data.processes.metadata.metafbtitle + ''  )
		meta(name='twitter:title', content='' + data.processes.metadata.metatwittertitle + '' )
		meta(name='description' content='' + data.processes.metadata.metadescription + '' )
		meta(property='og:description' , content='' + data.processes.metadata.metafbdescription + '' )
		meta(name='twitter:description', content='' + data.processes.metadata.metatwitterdescription + '' )
