extends ../layouts/default
include ../mixins/cta
block metadata
	if  data.aboutus
		title= data.aboutus.metadata.metatitle || 'Profile Pension'
		meta(property='og:title' , content='' + data.aboutus.metadata.metafbtitle || data.aboutus.title + ''  )
		meta(name='twitter:title', content='' + data.aboutus.metadata.metatwittertitle || data.aboutus.title  + '' )
		meta(name='description' content='' + data.aboutus.metadata.metadescription + '' )
		meta(property='og:description' , content='' + data.aboutus.metadata.metafbdescription + '' )
		meta(name='twitter:description', content='' + data.aboutus.metadata.metatwitterdescription + '' )
		meta(property='og:image' content='' + data.aboutus.metadata.metafbimageurl + '' )
		meta(name='twitter:image:src' content='' + data.aboutus.metadata.metatwitterdescription + '' )
		meta(name='twitter:card' , content='' + data.aboutus.metadata.metatwittercard + '' )
		meta(name='keywords' content='' + data.aboutus.metadata.metakeywords + '' )
	else
		+postmeta()

block intro
	section.intro.green.text-center
		h1= data.aboutus.title
		h3= data.aboutus.longtitle
	div.grey
		ul.container.breadcrumb
			li= data.aboutus.breadcrumbstitle
block content
	section.p3rem
		.container.text-center
			.row
				.col-md-6.col-sm-12
					h2= data.aboutus.s1title
					p!= data.aboutus.s1content
				.col-md-6.col-sm-12
					a.col-md-12(href="https://www.youtube.com/embed/B0s4Ef1kEeY?rel=0" data-lity="")
						img.img-responsive.col-md-12(src="https://www.profilepensions.co.uk/wp-content/uploads/2016/09/Video-Thumbnail.jpg")


	section.grey.p3rem
		.container.text-center
			.row
				.col
					h2= data.aboutus.s2title
					p.text-center!= data.aboutus.s2content   
			.row.text-center.p3rem
				.col
					div.circle-green
						img(height='30px' src="data:image/png;base64," alt ="Unique Pension Advice")
					h3= data.aboutus.s2feature1
					p= data.aboutus.s2feature1text
				.col
					div.circle-green
						img(height='30px' src="data:image/png;base64," alt ="Unique Pension Advice")
					h3= data.aboutus.s2feature2
					p= data.aboutus.s2feature2text
				.col
					div.circle-green
						img(height='30px' src="data:image/png;base64, " alt ="Unique Pension Advice")
					h3= data.aboutus.s2feature3
					p= data.aboutus.s2feature3text
				.col
					div.circle-green
						img(height='30px' src="data:image/png;base64, " alt ="Unique Pension Advice")
					h3= data.aboutus.s2feature4
					p= data.aboutus.s2feature4text
			.col-md-12
				a.btn.btn-lg.btn-faq(href='/pensions-faqs')= data.aboutus.s2button	
	section.green.p3rem
		.container.text-center
			.row
			.col-md-12
				h2.text-white= data.aboutus.s3title
				p!= data.aboutus.s3content
				a.btn.btn-lg.btn-faq(href='/meet-the-team')= data.aboutus.s3button
	section#page-cta.grey.p3rem
		.container.text-center
			.row
				.col-md-12
					h2= data.aboutus.s4title
					p.text-center!= data.aboutus.s4content
					a.btn.btn-lg.btn-primary(href='/get-started-trace', role='button')= data.aboutus.s4button
